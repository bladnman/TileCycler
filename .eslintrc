{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "no-debugger": 0,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-unused-expressions": 2,
    "guard-for-in": 0,
    "wrap-iife": 0,
    "no-use-before-define": 2,
    "linebreak-style": 0,
    "comma-style": [
      2,
      "last"
    ],
    "max-depth": [
      2,
      5
    ],
    "max-params": [
      2,
      4
    ],
    "max-statements": [
      2,
      42
    ],
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-undef": 2,
    "dot-notation": 0,
    "strict": 0,
    "no-unused-vars": 2
  }
}
